@model List<ReDoMusic.Domain.Entities.Customer>

<h1>Customer List</h1>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.FirstName</td>
                <td>@customer.LastName</td>
                <td>@customer.Email</td>
                <td>@customer.Address</td>
                <td>
                    <a href="@Url.Action("Edit", "Customer", new { id = customer.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", "Customer", new { id = customer.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
