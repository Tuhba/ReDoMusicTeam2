@model List<ReDoMusic.Domain.Entities.Order>

<h1>Order List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Order Id</th>
            <th> OrderStatus</th>
            <th>OrderDate</th>
            <th>DeliveryDate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.OrderStatus</td>
                <td>@order.OrderDate</td>
                <td>@order.DeliveryDate</td>
                <td>
                    <a href="@Url.Action("Edit", "Order", new { id =order.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", "Order", new { id = order.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
